iveName]) \n\n    // FetchCards ended\n    function handleName(each){\n        console.log(each)\n        setActiveName(each)\n    }\n\n    //  isLoding \n    if(isLoading){\n        <img src=\"/images/loading.gif\" alt=\"\" />\n    }\n    return (\n        <>\n            <div className={darkMode === true ? \"flex tags justify_between text_white\" : \"flex tags justify_between\"}>\n                {\n                    tags.map((each) => (\n                        <h3 className={activeName === each ? 'active' : \"\"} onClick={() => handleName(each)}>{each}</h3>\n                    ))\n                }\n            </div>\n            <div>\n                {\n                    FetchCards ? <Card darkMode={darkMode} FetchCards={FetchCards}/> : <img src=\"/images/loading.gif\" alt=\"\" /> \n                }\n            </div>\n\n        </>\n    )\n}\n\nexport default FetchAllCards\n","import React from 'react';\nimport {NavLink} from 'react-router-dom'\n\nfunction Header(props) {\n    var {darkModeset,darkMode,whiteModeset} = props;\n    return (\n        <header className={darkMode === true ? \" darkcolor\" : \"\"}>\n            <div className=\"container header\">\n                <nav className=\" flex justify_between\">\n                    <ul className=\"flex items_center\">\n                        <li><h3>\n                            <NavLink to=\"/\" activeClassName=\"active\">Popular</NavLink>\n                        </h3></li>\n                        <li><h3>\n                            <NavLink to=\"/battle\" activeClassName=\"active\">Battle</NavLink>    \n                        </h3></li>\n                    </ul>\n\n                    <figure className=\"brighter\">\n                        {/* {darkMode === true ? <img onClick={whiteModeset} src=\"https://cdn-icons.flaticon.com/png/512/881/premium/881115.png?token=exp=1635275806~hmac=c987fbcc0b27dc7d48da960471a5092a\" alt=\"\" /> : <img  onClick={darkModeset} src=\"https://cdn-icons.fla